    "jimp": "^0.22.10",
    "lint-staged": "^14.0.1",
    "node-fetch-cache": "^3.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "sharp": "^0.32.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "~4.8.4"
  },
  "packageManager": "yarn@3.6.3",
  "engines": {
    "node": ">=16.0.0"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@metamask/snaps-controllers>@metamask/permission-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>keccak": false,
      "@metamask/snaps-controllers>@metamask/permission-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>secp256k1": false,
      "gatsby": false,
      "gatsby>@parcel/cache>lmdb": false,
      "gatsby>@pmmmwh/react-refresh-webpack-plugin>core-js-pure": false,
      "gatsby>core-js": false,
      "gatsby>gatsby-cli": false,
      "gatsby>gatsby-sharp>sharp": true,
      "gatsby>gatsby-telemetry": false,
      "gatsby>lmdb": false,
      "gatsby>lmdb>msgpackr>msgpackr-extract": false,
      "gatsby>memoizee>es5-ext": false,
      "gatsby-plugin-manifest>sharp": true,
      "@lingui/cli>esbuild": false,
      "simple-git-hooks": true,
      "gatsby-plugin-sharp>sharp": true,
      "sharp": true,
      "@metamask/snaps-utils>@metamask/permission-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>keccak": false,
      "@metamask/snaps-utils>@metamask/permission-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>secp256k1": false,
      "wagmi>@wagmi/core>@wagmi/connectors>@coinbase/wallet-sdk>keccak": false,
      "@wagmi/core>@wagmi/connectors>@coinbase/wallet-sdk>keccak": false
