    "@reduxjs/toolkit": "^1.9.6",
    "@wagmi/core": "^1.4.10",
    "connectkit": "^1.5.3",
    "crypto-browserify": "^3.12.0",
    "framer-motion": "^10.16.2",
    "gatsby": "^5.12.3",
    "linkify-react": "^4.1.1",
    "linkifyjs": "^4.1.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-use-fusejs": "^2.0.2",
    "redux": "^4.2.1",
    "semver": "^7.5.4",
    "viem": "^1.19.13",
    "wagmi": "^1.4.10"
